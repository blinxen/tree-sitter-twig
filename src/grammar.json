          "type": "STRING",
          "value": "{#"
        },
        {
          "type": "PATTERN",
          "value": "[^#]*\\#+([^\\}#][^#]*\\#+)*"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    }
