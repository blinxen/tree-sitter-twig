          {
            "type": "SYMBOL",
            "name": "block_directive"
          },
          {
            "type": "SYMBOL",
            "name": "variable_directive"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "content"
          }
        ]
      }
    },
    "content": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "PATTERN",
          "value": "[^\\{]+|\\{"
        }
      }
    },
    "block_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
