              "type": "STRING",
              "value": "{%"
            },
            {
              "type": "STRING",
              "value": "{%-"
            },
            {
              "type": "STRING",
              "value": "{%~"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "block_code"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "%}"
            },
            {
              "type": "STRING",
              "value": "-%}"
            },
            {
              "type": "STRING",
              "value": "~%}"
            }
          ]
        }
      ]
    },
    "block_code": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "[^%-~]+|[%-~]"
      }
    },
    "variable_directive": {
